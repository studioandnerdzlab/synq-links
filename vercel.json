{
  "headers": [
    {
      "source": "/.well-known/apple-app-site-association",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
    "rewrites": [
         {
      "source": "/.well-known/(.*)",
      "destination": "/well-known/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
